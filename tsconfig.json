{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "es2024",
    "lib": ["es2024"],
    "rootDir": ".",
    "outDir": "dist",
    "strict": true
  }
}
