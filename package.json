{
  "name": "@himenon/template-esm-js",
  "version": "1.2.13",
  "description": "ESModule Library Template",
  "keywords": [
    "template",
    "typescript"
  ],
  "bin": {
    "himenon-template-esm-js": "./dist/cli.cjs"
  },
  "homepage": "https://github.com/Himenon/template-esm-js#readme",
  "bugs": {
    "url": "https://github.com/Himenon/template-esm-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Himenon/template-esm-js.git"
  },
  "license": "MIT",
  "author": {
    "name": "Himenon",
    "email": "k.himeno314@gmail.com",
    "url": "https://github.com/Himenon"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "src",
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "cli": "tsx ./src/cli.ts",
    "build": "pnpm run clean && rollup -c",
    "clean": "rimraf dist",
    "lerna:version:up": "lerna version --yes",
    "release:github:registry": "pnpm publish --registry https://npm.pkg.github.com/:_authToken=${NODE_AUTH_TOKEN}",
    "release:npm:registry": "pnpm publish",
    "test": "run-p test:vitest lint",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "validate": "biome check --write .",
    "test:vitest": "vitest"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,json,yml,yaml,html,md,ts,tsx}": [
      "biome format --no-errors-on-unmatched --write"
    ]
  },
  "packageManager": "pnpm@10.16.1",
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.1.3",
    "@swc/core": "^1.12.1",
    "@types/node": "24.1.0",
    "commander": "^14.0.0",
    "conventional-changelog-angular-all": "1.7.0",
    "cross-env": "^10.0.0",
    "lerna": "8.2.3",
    "lint-staged": "16.2.3",
    "npm-run-all": "4.1.5",
    "rimraf": "^6.0.1",
    "rollup": "^4.43.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "simple-git-hooks": "^2.13.0",
    "tsx": "^4.20.3",
    "typescript": "5.8.3",
    "vitest": "^3.2.4",
    "zod": "^4.0.5"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "publishConfig": {
    "access": "public"
  }
}
